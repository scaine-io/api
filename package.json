{
	"name": "api",
	"version": "1.0.0",
	"description": "image background removal",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc",
		"start": "node dist/index.js",
		"watch": "nodemon --watch src --ext ts --exec ts-node-dev --respawn --transpile-only src/index.ts",
		"deploy": "tsc && gcloud run deploy api --source . --platform managed --region europe-west1 --allow-unauthenticated --memory 512Mi --cpu 1 --max-instances 100 --min-instances=1 --set-env-vars $(cat .env | grep -v '^#' | xargs | sed 's/ /,/g')",
		"test": "nodemon --ext ts --watch . --exec \"ts-node test-process.ts\"",
		"clean": "rm -rf dist",
		"prebuild": "npm run clean",
		"depcheck": "npx depcheck",
		"update": "ncu -u @scaine-io/* && npm install",
		"unused": "ts-prune"
	},
	"dependencies": {
		"@scaine-io/shared": "^1.0.5",
		"@scaine-io/types": "^0.0.14",
		"@types/form-data": "^2.5.2",
		"axios": "^1.11.0",
		"cors": "^2.8.5",
		"dotenv": "^16.6.1",
		"express": "^4.18.2",
		"form-data": "^4.0.4",
		"multer": "^2.0.2"
	},
	"devDependencies": {
		"@types/axios": "^0.14.4",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/node": "^20.10.5",
		"@typescript-eslint/eslint-plugin": "^6.15.0",
		"@typescript-eslint/parser": "^6.15.0",
		"concurrently": "^9.2.0",
		"eslint": "^8.56.0",
		"firebase-admin": "^13.4.0",
		"node-fetch": "^3.3.2",
		"nodemon": "^3.1.10",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.3.3"
	}
}
